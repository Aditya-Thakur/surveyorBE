{
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "access-control-allow-origin",
                    "value": "*"
                }
            ],
            "routes": [
                {
                    "src": "/(.*)",
                    "dest": "/"
                }
            ],
            "builds": [
                {
                    "src": "./index.js",
                    "use": "@vercel/node"
                }
            ],
        }
    ]
}