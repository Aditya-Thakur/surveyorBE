{
    "name": "surveyor-be",
    "version": 2,
    "env": {
      "MONGODB_URI": "@my-mongodb-uri"
    },
    "builds": [
        {
            "src": "./server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
  }